<h1>Restaurants</h1>

<%= link_to "Create", new_restaurant_path %>


<ul>
  <% @restaurants.each do |restaurant| %>
    <li><%= restaurant.name %></li>
    <%= link_to "Show", restaurant_path(restaurant) %>
    <%= link_to "Update", edit_restaurant_path(restaurant[:id]) %>
    <%= link_to "Review", new_restaurant_review_path(restaurant[:id]) %>
  <% end %>
</ul>


